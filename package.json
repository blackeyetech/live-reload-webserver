{
  "description": "Live reload webserver",
  "scripts": {
    "start": "gulp start",
    "check-staged": "lint-staged",
    "format": "prettier --write '**/*.{js,json,md}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "prettier": {
    "trailingComma": "all"
  },
  "keywords": [],
  "author": "Black Eye Technology",
  "license": "MIT",
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4"
  },
  "dependencies": {
    "connect-livereload": "^0.6.1",
    "express": "^4.16.4",
    "gulp": "^4.0.0",
    "gulp-livereload": "^4.0.1"
  },
  "version": "0.0.0"
}
